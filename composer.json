    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/silvercommerce/catalogue-admin",
    "keywords": [
        "silverstripe",
        "commerce",
        "ecommerce",
        "subsites"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Morven Lewis-Everley",
            "email": "morven@ilateral.co.uk"
        },
        {
            "name": "Mark Anderson",
            "email": "mark@ilateral.co.uk"
        }
    ],
    "support": {
        "issues": "https://github.com/silvercommerce/subsites/issues"
    },
    "require": {
        "silverstripe/framework": "~4.0",
        "silverstripe/subsites": "~2.0"
    },
    "autoload": {
